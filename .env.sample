# Copy this file to `.env` and adjust values for your environment.
# Environment variables are consumed by backend.app.core.config.Settings.

# Local Postgres connection string. Defaults to peer authentication using your OS user.
DATABASE_URL=postgresql+asyncpg:///batumi_lunch
# Redis connection used for rate limiting, caching, and background jobs.
REDIS_URL=redis://localhost:6379/0
# Secrets for signing tokens or encrypting sensitive data.
SECRET_KEY=change-me
# Optional: override logging level (DEBUG, INFO, WARN, ERROR)
LOG_LEVEL=INFO
